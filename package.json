{
  "devDependencies": {
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "mineflayer": "^4.8.1",
    "mineflayer-auto-eat": "^3.3.1",
    "mineflayer-autoclicker": "^1.2.0",
    "mineflayer-pathfinder": "^2.4.2",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.22",
    "pngjs-image": "^0.11.7"
  },
  "name": "sapok-client",
  "version": "1.1.3",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "hello",
  "build": {
    "appId": "SapokClient",
    "productName": "SapokClient",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/s-brown.ico"
    },
    "nsis": {
      "installerIcon": "assets/s-brown.ico",
      "uninstallerIcon": "assets/s-brown.ico",
      "installerSidebar": "assets/installer-img.bmp",
      "uninstallerSidebar": "assets/installer-img.bmp",
      "uninstallDisplayName": "SapokClient-uninstaller",
      "license": "license.md",
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "runAfterFinish": true,
      "perMachine": true
    }
  }
}
