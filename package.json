{
  "devDependencies": {
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "mineflayer": "^4.8.1",
    "mineflayer-autoclicker": "^1.2.0",
    "mineflayer-pathfinder": "^2.4.2",
    "pngjs-image": "^0.11.7"
  },
  "name": "sapok-client",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "nodemon": "nodemon --watch . --exec \"electron .\"",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "hello",
  "build": {
    "appId": "SapokClient",
    "productName": "SapokClient",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/s-brown.ico"
    },
    "nsis": {
      "installerIcon": "assets/s-brown.ico",
      "uninstallerIcon": "assets/s-brown.ico",
      "installerSidebar": "assets/setup-img.bmp",
      "uninstallerSidebar": "assets/setup-img.bmp",
      "uninstallDisplayName": "SapokClient-uninstaller",
      "license": "license.md",
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "runAfterFinish": false,
      "perMachine": true
    }
  }
}